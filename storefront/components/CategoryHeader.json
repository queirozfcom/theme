{
  "intention": "header",
  "resourceBindings": [
    {
      "locator": "search@vtex.storefront-sdk",
      "relativePath": "/{{ account }}/facets",
      "queryParams": {
        "category": "{{ route.splat }}",
        "brands": "{{ query.brands }}"
      },
      "bindTo": "facets@vtex"
    }
  ],
  "areas": [
    {
      "id": "category/category-header/filter-panel",
      "intention": "filter-panel"
    },
    {
      "id": "category/category-header/filter-button",
      "intention": "filter-button"
    }
  ],
  "assets": [
    "common.js",
    "CategoryHeader.js"
  ]
}
