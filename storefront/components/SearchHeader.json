{
  "intention": "header",
  "resourceBindings": [
    {
      "locator": "search@vtex.storefront-sdk",
      "relativePath": "/{{ account }}/facets",
      "queryParams": {
        "query": "{{ route.searchTerm }}"
      },
      "bindTo": "facets@vtex"
    }
  ],
  "areas": [
    {
      "id": "search/search-header/filter-button",
      "intention": "filter-button"
    },
    {
      "id": "search/search-header/filter-panel",
      "intention": "filter-panel"
    }
  ],
  "assets": [
    "common.js",
    "SearchHeader.js"
  ]
}
